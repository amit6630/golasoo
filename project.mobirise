          "_customHTML": "<section data-bs-version=\"5.1\" group=\"Footers\" class=\"footer1 medicalm4_footer1\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked=\"\">\n        <header>Colors</header>\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#4f87fb\" selected=\"\" condition=\"showLine\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconcolor\" value=\"#ffffff\" selected=\"\">\n        <input type=\"color\" title=\"Icons Bg Color\" name=\"iconBgcolor\" value=\"#4f87fb\" selected=\"\">\n        <input type=\"color\" title=\"Link Icons Color\" name=\"linkiconcolor\" value=\"#ffffff\" selected=\"\">\n        <input type=\"color\" title=\"Days Color\" name=\"daysColor\" value=\"#ffffff\" selected=\"\">\n        <input type=\"color\" title=\"Times Color\" name=\"timesColor\" value=\"#4f87fb\" selected=\"\">\n        <input type=\"color\" title=\"Links Hover\" name=\"linksHoverColor\" value=\"#4f87fb\" selected=\"\">\n        <hr>\n        <input type=\"range\" inline=\"\" title=\"Items\" name=\"items\" min=\"1\" max=\"5\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Links\" name=\"links\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Hours Items\" name=\"hours\" min=\"1\" max=\"8\" step=\"1\" value=\"4\">\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1-2.jpg\">\n            